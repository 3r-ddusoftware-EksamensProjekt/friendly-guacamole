<!DOCTYPE html>
<html>
    <body>
        <div class="container">
            <h2>Database Results</h2>

            <ul>
                <% if(results) { results.forEach(function(row) { %>
                <li><%= row.first_name %> <%= row.last_name %></li>
                <% }) }; %>
            </ul>
        </div>
    </body>
</html>
